{
  "name": "dev-portfolio",
  "version": "1.0.0",
  "description": "Portfolio with React and Express.",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "node ./src/server",
    "server": "nodemon ./src/server",
    "client": "react-scripts start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build:client": "react-scripts build",
    "build:server": "NODE_ENV=production ./node_modules/.bin/webpack --mode=production --config webpack.server.js",
    "build": "npm-run-all --parallel build:*",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lifeanddoodles/dev-portfolio.git"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app",
    "portfolio",
    "ssr"
  ],
  "author": "Sandra Vargas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lifeanddoodles/dev-portfolio/issues"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "gh-pages": "^2.1.1",
    "nodemailer": "^6.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-router-hash-link": "^1.2.1",
    "react-scripts": "^3.0.1",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-preset-react-app": "^9.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": "10.x"
  },
  "homepage": "https://lifeanddoodles.github.io/dev-portfolio",
  "proxy": "http://localhost:5000"
}
